# FileMaker Data API Configuration
# Copy this file to .env.local and update with your FileMaker Server details

# FileMaker Server hostname (without https://)
REACT_APP_FILEMAKER_SERVER=your-filemaker-server.com

# FileMaker Database name
REACT_APP_FILEMAKER_DATABASE=AuditPro

# FileMaker API User credentials
# Create a dedicated user in FileMaker with API access
REACT_APP_FILEMAKER_USERNAME=api_user
REACT_APP_FILEMAKER_PASSWORD=secure_password

# Optional: Custom Data API port (default is 443)
# REACT_APP_FILEMAKER_PORT=443

# Optional: Enable SSL verification (default is true)
# REACT_APP_FILEMAKER_SSL_VERIFY=true

# Development mode settings
REACT_APP_DEVELOPMENT_MODE=true
REACT_APP_MOCK_DATA=false
